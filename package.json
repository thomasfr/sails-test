{
  "name": "sails-aws",
  "private": true,
  "version": "1.0.0",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "connect-redis": "^3.1.0",
    "ejs": "2.3.4",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-coffee": "1.0.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-cssmin": "1.0.1",
    "grunt-contrib-jst": "1.0.0",
    "grunt-contrib-less": "1.3.0",
    "grunt-contrib-uglify": "1.0.1",
    "grunt-contrib-watch": "1.0.0",
    "grunt-sails-linker": "~0.10.1",
    "grunt-sync": "0.5.2",
    "hiredis": "0.5.0",
    "include-all": "^1.0.0",
    "rc": "1.0.1",
    "redis": "2.6.3",
    "sails": "~0.12.8",
    "sails-mysql": "^0.11.5",
    "sails-redis": "0.10.7",
    "socket.io-redis": "^1.1.1"
  },
  "scripts": {
    "debug": "node debug app.js",
    "start": "node app.js",
    "start:cluster": "node cluster.js",
    "grunt": "grunt",
    "www": "sails www",
    "sails": "sails",
    "build": "npm run www",
    "package": "zip ./.tmp/build.zip -r * .[^.]* -x 'node_modules/*' -x 'npm-debug.log' -x '*.pid' -x '*.out' -x '*.log' -x '*.seed' -x 'lib-cov' -x 'dump.rdb' -x '.node-history' -x '*~' -x '*#' -x '.elasticbeanstalk/*' -x '.tmp/*' -x '.idea/*' -x '.git/*' -x '.gitignore' -x 'assets/*' -x 'tasks/*'",
    "deploy": "npm run build && npm run package && eb deploy"
  },
  "main": "app.js",
